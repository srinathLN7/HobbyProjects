<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= homeContent %></p>


<% let postArray = postsEJS; %>
<% postArray.forEach(function(post) { %>
    <h1> <%= post.postTitle %> </h1>
    <p>
    <%= post.postBody.substring(0, 100) + "..." %>
    <a href="/posts/<%= post._id %>"> Read More</a>
    </p>
<% });%>


<%- include("partials/footer"); -%>
